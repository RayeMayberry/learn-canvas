ion, distance, speed) {\n        if (direction === 'left') {\n            this.index[1] = 0;\n        }\n        if (direction === 'up') {\n            this.index[1] = 1;\n        }\n        if (direction === 'down') {\n            this.index[1] = 2;\n        }\n        if (direction === 'right') {\n            this.index[1] = 3;\n        }\n\n        setInterval(\n            () => {\n                if (this.position[0] < distance[0]) {\n                    this.position[0]++;\n                }\n                if (this.position[1] < distance[1]) {\n                    this.position[1]++;\n                }\n            }, 1000 / speed\n        )\n        return this;\n    }\n};\n"},"lineCount":null}},"error":null,"hash":"06a7f247819d5d298782e248a60aac92","cacheData":{"env":{}}}